k8s:
  dns: kube-dns.kube-system
  persistence:
    enabled: false

whisk:
  ingress:
    apiHostName: {{ API_HOST_NAME }}

controller:
  edge: true
  imageName: "binw/controller"
  imageTag: "latest"
  imagePullPolicy: "Always"

utility:
  imageName: "binw/ow-utils"
  imageTag: "latest"
  imagePullPolicy: "Always"

invoker:
  imageName: "binw/invoker"
  imageTag: "latest"
  imagePullPolicy: "Always"
  containerFactory:
    impl: "docker"
